{
  "name": "notion2anki",
  "description": "Go from Notion togglelists to Anki Cards easily!",
  "keywords": [
    "notion",
    "anki",
    "notion2anki"
  ],
  "author": "Alexander Alemayhu",
  "version": "0.3.0-0",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --mode=development --content-base dist/",
    "watch": "webpack --watch --progress --colors --display-modules",
    "test": "ava",
    "start": "yarn dev",
    "build": "tsc -p .",
    "dev-server": "nodemon --watch 'server/**/*.ts' --exec \"ts-node\" server/server.ts",
    "lint": "tslint -c tslint.json 'server/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alemayhu/notion2anki.git"
  },
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "express": "^4.17.1",
    "find-remove": "^2.0.3",
    "jszip": "^3.5.0",
    "knex": "^0.21.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12"
  },
  "build": {
    "appId": "com.alemayhu.notion2anki",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "main.js",
      "dist/app.js",
      "dist/index.html",
      "build",
      "node_modules"
    ]
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.28",
    "@types/express": "^4.17.11",
    "@types/find-remove": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.36",
    "ava": "^3.15.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.1"
  }
}
