    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/find-remove": "^2.0.0",
    "@types/html-to-text": "^8.1.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.5.10",
    "@types/react-dom": "17.0.11",
    "dotenv": "^10.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
