{
  "name": "notion2anki-server",
  "description": "Go from Notion togglelists to Anki Cards easily!",
  "keywords": [
    "notion",
    "anki",
    "notion2anki"
  ],
  "author": "Alexander Alemayhu",
  "version": "0.7.6",
  "engines": {
    "node": ">=12.0.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "main": "main.js",
  "scripts": {
    "test": "ava --timeout=121s",
    "start": "yarn dev",
    "build": "tsc -p .",
    "dev": " WORKSPACE_BASE=/tmp/w nodemon --watch '**/*.ts' --exec \"ts-node\" server.ts",
    "lint": "tslint -c tslint.json '**/*.ts'",
    "purge-js": "rm `find . -name '*.js'|grep -v node_modules`",
    "migrate": "knex migrate:latest --env production"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alemayhu/notion2anki.git"
  },
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^0.1.9",
    "@sentry/node": "^6.12.0",
    "@sentry/react": "^6.12.0",
    "@sentry/tracing": "^6.12.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/multer-s3": "^2.7.10",
    "aws-sdk": "^2.991.0",
    "base-64": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "find-remove": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.7.0",
    "knex": "^0.95.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nanoid": "^3.1.12",
    "node-fetch": "^2.6.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/cheerio": "^0.22.28",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/find-remove": "^2.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.5.10",
    "ava": "^3.15.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
