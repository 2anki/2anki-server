{
  "name": "n2a",
  "description": "Go from Notion toggle list to Anki Cards easily!",
  "keywords": [
    "notion",
    "anki",
    "notion2anki",
    "imba",
    "electron"
  ],
  "author": "Alexander Alemayhu",
  "version": "0.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "main.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack-dev-server --mode=development --content-base dist/",
    "package-linux": "electron-packager . 'Notion 2 Anki' --platform=linux",
    "package-osx": "electron-packager . 'Notion 2 Anki' --platform=darwin",
    "package-windows": "electron-packager . 'Notion 2 Anki' --platform=win32",
    "start": "electron .",
    "watch": "webpack --watch --progress --colors --display-modules"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alemayhu/notion2anki.git"
  },
  "license": "MIT",
  "dependencies": {
    "imba": "^2.0.0-alpha.45"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "css-loader": "^1.0.1",
    "electron": "^6.0.0",
    "electron-packager": "^14.0.5",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.4.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.1"
  }
}
